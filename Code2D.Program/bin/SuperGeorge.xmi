<?xml version="1.0" encoding="ASCII"?>
<Code2D:Game
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:Code2D="http://www.utwente.nl/apc/Code2D"
    xsi:schemaLocation="http://www.utwente.nl/apc/Code2D ../../nl.utwente.apc.Code2D/metamodel/text.ecore"
    Name="SuperGeorge">
  <GameObjects
      xsi:type="Code2D:NPC"
      Name="RedMonster"
      Behaviour="HOSTILE"
      Extends="//@GameObjects.2"/>
  <GameObjects
      xsi:type="Code2D:Scenery"
      Name="Tree"
      Texture="tree1.png"/>
  <GameObjects
      xsi:type="Code2D:NPC"
      Name="BlueMonster"
      Behaviour="HOSTILE"/>
  <GameObjects
      xsi:type="Code2D:Terrain"
      Name="Grass"/>
  <GameWorld
      Name="Level1">
    <WorldTriggers
        xsi:type="Code2D:CollisionTrigger"
        Name="GeorgeHitsATree"
        Object1="//@Player"
        Object2="//@GameObjects.1"/>
    <WorldEvents
        Name="GameOver"
        CollisionTrigger="//@GameWorld/@WorldTriggers.0">
      <EventActions
          xsi:type="Code2D:BasicAction"/>
    </WorldEvents>
    <PlayerInstance
        X1="12"
        Y1="12"
        Player="//@Player"/>
    <WorldInstances
        X2="12"
        Y2="11"
        Object="//@GameObjects.1"/>
  </GameWorld>
  <Player Name="Super George"
      Texture="supergeorge.png"/>
</Code2D:Game>
